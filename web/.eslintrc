{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
  ],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "@typescript-eslint/no-explicit-any": ["off"]
  },
  "rules": {
    "no-console": "off", // 不禁止console
    "@typescript-eslint/no-explicit-any":"off"
  },
  "root": true
}