{
    "name": "laynnelcms",
    "version": "0.0.1",
    "description": "A simple theme for LaynelCMS",
    "author": "LaynelCMS",
    "license": "MIT",
    "homepage": "https://github.com/LaynelCMS/theme001",
    "repository": {
        "type": "git",
        "url": "https://github.com/LaynelCMS/theme001.git"
    },
    "keywords": [
        "LaynelCMS",
        "core"
    ],
    "pages": [{
            "name": "dashboard",
            "title": "Dashboard",
            "url": "/dashboard",
            "icon": "fa:dashboard",
            "component": "project.base.index"
        },
        {
            "name": "settings",
            "title": "Settings",
            "url": "/settings",
            "icon": "fa:setting",
            "redirect": "/users/list",
            "children": [{
                    "name": "users",
                    "title": "Users",
                    "url": "/settings/users",
                    "icon": "fa:users",
                    "component": "project.user.index"
                }, {
                    "name": "roles",
                    "url": "Roles",
                    "path": "/settings/roles",
                    "icon": "fa:role",
                    "component": "project.role.index"
                }

            ]
        },
        {
            "name": "plugins",
            "title": "Plugins",
            "url": "/plugins",
            "icon": "fa:plug",
            "component": "project.plugin.index"
        }
    ]
}