{
    "name": "laynnelcms",
    "version": "0.0.1",
    "description": "A simple theme for LaynelCMS",  
    "author": "LaynelCMS",
    "license": "MIT",
    "homepage": "https://github.com/LaynelCMS/theme001",
    "repository": {
        "type": "git",
        "url": "https://github.com/LaynelCMS/theme001.git"
    },
    "keywords": [
        "LaynelCMS",
        "core"
    ],
    "pages": [
        [
            {
                "name": "dashboard",
                "title": "Dashboard",
                "path": "/dashboard",
                "icon": "fa:dashboard",
                "component": "dashboard"
            },
            {
                "name": "settings",
                "title": "Settings",
                "path": "/settings",
                "icon": "fa:setting",
                "redirect": "/users/list",
                "children": [
                    {
                        "name": "users",
                        "title": "Users",
                        "path": "/settings/users",
                        "icon": "fa:users",
                        "component":"user"
                    },{
                        "name": "roles",
                        "title": "Roles",                
                        "path": "/settings/roles",
                        "icon": "fa:role",
                        "component":"user"
                    }
        
                ]
            },
            {
                "name":"plugins",
                "title":"Plugins",
                "path":"/plugins",
                "icon":"fa:plug"
            }
        ]
    ]
}